{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/main.js",
			"stopOnEntry": false,
			"args": [],
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
                "ATOM_SHELL_INTERNAL_RUN_AS_NODE": "0"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}